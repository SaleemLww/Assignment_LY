# ========================================
# AI/LLM API KEYS (Required)
# ========================================
# OpenAI API Key - Required for AI-powered timetable extraction
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Claude API Key - Alternative to OpenAI
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Deepseek API Key - Cost-effective AI vision processing
DEEPSEEK_API_KEY=your_deepseek_api_key_here
# ========================================
# GOOGLE API CONFIGURATION (Optional)
# ========================================
# Google Cloud Vision API for enhanced OCR
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_SERVICE_ACCOUNT_JSON=path_to_your_google_service_account_json_file_here

WHICH_OCR_KEY=OPENAI_API_KEY 

# ========================================
# SERVER CONFIGURATION
# ========================================
# Backend Server Port (Port 5001 recommended to avoid macOS ControlCenter conflict on port 5000)
PORT=5001
NODE_ENV=development

# ========================================
# LANGSMITH CONFIGURATION (Optional)
# ========================================
# For AI evaluation and tracing
LANGSMITH_API_KEY=your_langsmith_api_key_here
LANGSMITH_TRACING=true
LANGSMITH_PROJECT=your_project_name_here



# Alternative Google API credentials (if using OAuth)
Google_API_KEY2=your_google_api_key_here
Google_Client_id2=your_google_client_id.apps.googleusercontent.com
Google_Client_secret2=your_google_client_secret_here

# ========================================
# PINECONE CONFIGURATION (Optional - Commented)
# ========================================
# Vector database for embeddings (if needed in future)
# Pinecone_API_Key=your_pinecone_api_key_here

# ========================================
# POSTGRESQL DATABASE CONFIGURATION (Required)
# ========================================
# PostgreSQL connection settings
DB_CONNECTION_POST=postgres
DB_HOST_POST=127.0.0.1
DB_PORT_POST=5432
DB_DATABASE_POST=your_database_name
DB_USERNAME_POST=your_username
DB_PASSWORD_POST=your_password

# Database URL for Prisma/Backend
# Format: postgresql://username:password@host:port/database
DATABASE_URL=postgresql://postgres@127.0.0.1:5432/timetable_db

# ========================================
# REDIS CONFIGURATION (Required)
# ========================================
# Redis for BullMQ job queue
REDIS_HOST=127.0.0.1
REDIS_PORT=6379

# ========================================
# FEATURE FLAGS
# ========================================
# Enable/disable agentic workflow (LangGraph-based processing, coming soon)
USE_AGENTIC_WORKFLOW=false
